{
	"name" : "coka/registrator-php-sdk",
	"type" : "library",
	"license" : "MIT",
	"description" : "Service registry bridge for Docker. Registrator automatically registers and deregisters services for any Docker container by inspecting containers as they come online. Registrator supports pluggable service registries, which currently includes Consul, etcd and SkyDNS 2.",
	"keywords" : [
		"Docker",
		"Service",
		"Discovery",
		"Consul"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Oka\\Registrator\\" : "src"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\Registrator\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=7.2",
		"symfony/http-foundation" : "^3.4 || ^4.0",
		"guzzlehttp/guzzle" : "^6.3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8.3"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.x.x-dev"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/registrator-php-sdk/blob/master/doc/index.md"
	}
}